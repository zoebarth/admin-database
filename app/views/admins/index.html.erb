<div class="container">
  <div class="row justify-content-center">
    <h1>List of Admin Users</h1>
  </div>

  <div class="row justify-content-center">
    <h4> <%= link_to "Create New Admin" %> </h4>
  </div>

  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Email </th>
      </tr>
    </thead>
    <tbody>
      <% @admins.each do |user| %>
        <tr>
          <td> <%= user.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>