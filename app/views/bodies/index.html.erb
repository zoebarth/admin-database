<div class="container">
  <div class="row justify-content-center">
    <h1>Admin List of Bodies of Water</h1>
  </div>

  <div class="row justify-content-center">
    <h4> <%= link_to "Create New Body of Water", new_body_path %> </h4>
  </div>


  <% @bodies.each do |body| %>
    <div class="row justify-content-center">
      <p> <%= link_to body.name, body_path(body)%> </p>
    </div>
  <% end %> 
</div>