<div class="container">
  <div class="row justify-content-center">
    <h1>Admin List of Fish</h1>
  </div>

  <div class="row justify-content-center">
    <h4> <%= link_to "Create new fish", new_fish_path %> </h4>
  </div>


  <% @fishes.each do |fish| %>
    <div class="row justify-content-center">
      <p> <%= link_to fish.name, fish_path(fish)%> </p>
    </div>
  <% end %>

</div>